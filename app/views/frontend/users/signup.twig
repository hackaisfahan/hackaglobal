{% extends "layouts/default.twig" %}

{% block content %}

        	<h1 class="content-header">فرم ثبت نام در هاکا اصفهان</h1>
            <form action="" method="post" id="signup-form" class="clear">
                <div class="form-obj">
                    <span class="require">ایمیل : *</span>
                    <input type="email" name="email" id="email"/>
                </div>
                <div class="form-obj">
                    <span class="require">رمز عبور : *</span>
	                <input type="password" name="password" id="password"/>
                </div>
                
                <div class="form-obj submit">                
	                <input type="submit" value="ثبت نام" class="signup"/>
                </div>
            </form>


{% endblock %}
